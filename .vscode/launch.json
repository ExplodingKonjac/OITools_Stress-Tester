{
	"configurations": [
		{
			"name": "(gdb) Launch Current File",
			"type": "cppdbg",
			"request": "launch",
			"program": "${file}",
			"args": [
				"test",
				"my",
				"-Xg,--n=20,--q=20,--seed-by-time",
				"-j2",
				"--compile-gen=false"
			],
			"stopAtEntry": false,
			"cwd": "${fileDirname}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "D:/msys64/ucrt64/bin/gdb.exe",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Set Disassembly Flavor to Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}
	]
}